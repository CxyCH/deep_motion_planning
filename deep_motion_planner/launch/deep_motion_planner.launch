<launch>
  <node name="deep_motion_planner" pkg="deep_motion_planner" type="deep_motion_planner_node.py"
  output="screen">
    <remap from="scan" to="base_scan"/>
    <param name="model_path" type="string" value="$(find deep_motion_planner)/models/2018-03-05_11-58_fc_36_shapes_10_dropout" />
    <param name="pickle_weights_path" type="string" value="$(find deep_motion_planner)/models/reinforcement_learning/weights_actor_il250+rl3maps.p" />
    <param name="use_pickle_weights" type="bool" value="True" />
    <param name="use_checkpoints" type="bool" value="False" />
    <param name="protobuf_file" type="string" value="model.pb" />
  </node>
</launch>
